{
    "manifest_version": 3,
    "name": "Focusy",
    "version": "1.0",
    "description": "Easy manage whitelist and blacklist browsing experience",
    "permissions": ["declarativeNetRequest", "storage"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "scripts/background.js"
    },
    "action": {
      "default_popup": "popup.html"
      
    }
}
  